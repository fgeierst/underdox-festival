{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Verdana, Geneva, Tahoma, sans-serif",
					"name": "Verdana",
					"slug": "verdana"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Nunito Sans",
							"fontStyle": "normal",
							"fontWeight": "200 1000",
							"src": [
								"file:./assets/fonts/nunito-sans_normal_200 1000.ttf"
							]
						}
					],
					"fontFamily": "Nunito Sans",
					"slug": "nunito-sans"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/post-terms": {
				"css": "a { text-decoration: none; }",
				"typography": {
					"textDecoration": "none"
				}
			},
			"core/site-title": {
				"typography": {
					"textDecoration": "none",
					"textTransform": "uppercase"
				}
			}
		},
		"css": "/* Theme Custom CSS start */\n/* Theme Custom CSS start */\n.subheader {\n margin-block-start: 0;\n}\n\n.subheader__term a, .page-title a { \ntext-decoration: none;\n}\n/* Theme Custom CSS end */\n/* Theme Custom CSS end */",
		"elements": {
			"heading": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "900"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|black"
				}
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|nunito-sans"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}