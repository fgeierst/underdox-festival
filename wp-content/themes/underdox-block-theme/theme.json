{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "620px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Verdana, Geneva, Tahoma, sans-serif",
					"name": "Verdana",
					"slug": "verdana"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/post-terms": {
				"css": "a { text-decoration: none; }",
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"css": "/* Theme Custom CSS start */\n.subheader {\n margin-block-start: 0;\n}\n\n.subheader__term a { \ntext-decoration: none;\n}\n/* Theme Custom CSS end */",
		"elements": {
			"link": {
				"color": {
					"text": "var:preset|color|black"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--verdana)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}